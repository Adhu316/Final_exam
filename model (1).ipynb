      "source": [
        "def one_hot_encoder(df, ohe_cols):\n",
        "    '''\n",
        "    One-Hot Encoder function\n",
        "    '''\n",
        "    print('Creating OHE features..\\nOld df shape:{}'.format(df.shape))\n",
        "    df = pd.get_dummies(df, columns=ohe_cols)\n",
        "    print('New df shape:{}'.format(df.shape))\n",
        "    return df"
      ]
    },
